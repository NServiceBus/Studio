<UserControl x:Class="NServiceBusStudio.Automation.CustomSolutionBuilder.Views.ToolbarExtension"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="16" d:DesignWidth="300">
    <StackPanel Orientation="Horizontal" Height="16">
        <Separator/>
        <Button Click="Button_Click" ToolTip="NServiceBus Studio View" x:Name="NServiceBusViewButton" Focusable="False">
            <BitmapImage UriSource="../../Resources/NServiceBusView.png" />
        </Button>
        <Button Click="Button_Click_1" ToolTip="Solution Builder Default View" Focusable="False">
            <BitmapImage UriSource="../../Resources/Hierarchy.png" />
        </Button>
        <Button Click="Button_Click_2" ToolTip="Get Help from NServiceBus Support Call" Focusable="False">
            <BitmapImage UriSource="../../Resources/Help.png" />
        </Button>
    </StackPanel>
</UserControl>
